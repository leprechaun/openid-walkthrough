{
  "name": "oauth-walkthrough",
  "version": "0.0.1",
  "description": "An OpenID Example",
  "main": "index.js",
  "repository": "https://github.com/leprechaun/openid-walkthrough",
  "author": "leprechaun",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "NODE_ENV='development' nodemon -e js,yml -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.js'",
    "audit": "yarn audit",
    "swagger:deref": "./node_modules/.bin/babel-node ./scripts/swagger-dereference.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.3.1",
    "express-prom-bundle": "^5.1.5",
    "express-winston": "^3.1.0",
    "jwks-rsa": "^1.5.0",
    "prom-client": "^11.3.0",
    "swagger-express-middleware": "^2.0.2",
    "yaml": "^1.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-recommended": "^4.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "jest-mock-express": "^0.1.1",
    "nodemon": "^1.18.7",
    "prettier": "1.14.3",
    "supertest": "^4.0.2",
    "swagger-cli": "^2.2.1"
  }
}
